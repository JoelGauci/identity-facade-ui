# App settings
SESSION_SECRET="mysessionsecret"
PORT=8082
REDIRECT_URI=http://localhost:8082
# Remember to update Apigee Application callback parameter to http://localhost:8082

# Insert here Apigee Application client ID and Secret
CLIENT_ID=xkey-xxxxx
CLIENT_SECRET=xsecret

# Insert here Apigeee URLs
ISSUER=https://example.com/v1/oauth20
DATA_URI=https://example.com/drive/v1/files

# PKCE settings
IS_PKCE_ENABLED=false

# Scopes
SCOPES=https://www.googleapis.com/auth/drive https://www.googleapis.com/auth/drive.appdata https://www.googleapis.com/auth/drive.file https://www.googleapis.com/auth/drive.metadata https://www.googleapis.com/auth/drive.metadata.readonly https://www.googleapis.com/auth/drive.photos.readonly https://www.googleapis.com/auth/drive.readonly https://www.googleapis.com/auth/drive.scripts openid
